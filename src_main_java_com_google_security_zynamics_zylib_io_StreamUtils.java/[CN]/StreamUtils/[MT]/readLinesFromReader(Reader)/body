{
  final BufferedReader br=new BufferedReader(reader);
  try {
    final List<String> lines=new ArrayList<String>();
    String line;
    while (true) {
      line=br.readLine();
      if (line == null) {
        return lines;
      }
      lines.add(line);
    }
  }
  finally {
    br.close();
  }
}
