{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bsoctrl");
  final IOperandTreeNode BIOperand=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bsoctrl",String.valueOf((Helpers.getCRRegisterIndex(BIOperand.getValue()) * 4) + 3),Helpers.COUNT_REGISTER,true,false,false,false,true,false);
}
