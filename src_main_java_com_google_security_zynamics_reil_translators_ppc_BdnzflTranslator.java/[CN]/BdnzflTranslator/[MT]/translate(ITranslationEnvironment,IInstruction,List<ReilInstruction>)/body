{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bdnrfl");
  final IOperandTreeNode addressOperand=instruction.getOperands().get(1).getRootNode().getChildren().get(0);
  final IOperandTreeNode BIOperand=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bdnzfl",BIOperand.getValue(),addressOperand.getValue(),true,false,true,false,true,true);
}
