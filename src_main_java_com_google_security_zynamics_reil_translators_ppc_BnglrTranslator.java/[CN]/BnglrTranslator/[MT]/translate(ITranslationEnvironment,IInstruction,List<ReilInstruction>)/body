{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bnglr");
  final IOperandTreeNode BIOperand=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bnglr",String.valueOf((Helpers.getCRRegisterIndex(BIOperand.getValue()) * 4) + 1),Helpers.LINK_REGISTER,false,false,false,false,true,true);
}
