{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bcl");
  final IOperandTreeNode addressOperand=instruction.getOperands().get(2).getRootNode().getChildren().get(0);
  GenericBranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bcl",addressOperand.getValue(),true,false);
}
