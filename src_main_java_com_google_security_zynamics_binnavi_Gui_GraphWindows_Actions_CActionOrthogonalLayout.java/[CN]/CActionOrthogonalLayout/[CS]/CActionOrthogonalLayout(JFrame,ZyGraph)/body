{
  super("Orthogonal Layout");
  m_parent=Preconditions.checkNotNull(parent,"IE02824: parent argument can not be null");
  m_graph=Preconditions.checkNotNull(graph,"IE02825: graph argument can not be null");
  putValue(Action.SMALL_ICON,new ImageIcon(CMain.class.getResource("data/layorth_up.jpg")));
  putValue(Action.SHORT_DESCRIPTION,"Orthogonal Layout");
}
