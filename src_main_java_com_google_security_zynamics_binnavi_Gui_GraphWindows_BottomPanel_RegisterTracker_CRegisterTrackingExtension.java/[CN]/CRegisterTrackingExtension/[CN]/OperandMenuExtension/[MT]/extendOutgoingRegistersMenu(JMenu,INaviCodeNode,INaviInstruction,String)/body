{
  menu.add(CActionProxy.proxy(new CTrackOperandAction(m_resultsContainer,instruction,register,new RegisterTrackingOptions(m_settingsPanel.doClearAllRegisters(),m_settingsPanel.getClearedRegisters(),false,AnalysisDirection.DOWN))));
  menu.add(CActionProxy.proxy(new CTrackOperandAction(m_resultsContainer,instruction,register,new RegisterTrackingOptions(m_settingsPanel.doClearAllRegisters(),m_settingsPanel.getClearedRegisters(),false,AnalysisDirection.UP))));
}
