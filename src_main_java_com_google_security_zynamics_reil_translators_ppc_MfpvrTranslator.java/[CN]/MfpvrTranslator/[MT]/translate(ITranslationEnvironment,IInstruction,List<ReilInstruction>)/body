{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"mfpvr");
  final IOperandTreeNode targetRegister=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  Long baseOffset=instruction.getAddress().toLong() * 0x100;
  final OperandSize dw=OperandSize.DWORD;
  instructions.add(ReilHelpers.createStr(baseOffset++,dw,"PVR",dw,targetRegister.getValue()));
}
