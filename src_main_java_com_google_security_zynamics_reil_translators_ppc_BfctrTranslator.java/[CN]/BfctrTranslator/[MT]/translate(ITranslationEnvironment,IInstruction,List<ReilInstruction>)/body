{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bfctr");
  final IOperandTreeNode BIOperand=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bfctr",BIOperand.getValue(),Helpers.COUNT_REGISTER,false,false,false,false,true,true);
}
