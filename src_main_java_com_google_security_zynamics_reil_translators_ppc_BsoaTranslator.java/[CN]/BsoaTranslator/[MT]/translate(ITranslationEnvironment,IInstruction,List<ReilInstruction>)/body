{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bsoa");
  final IOperandTreeNode addressOperand1=instruction.getOperands().get(1).getRootNode().getChildren().get(0);
  final IOperandTreeNode BIOperand=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bsoa",String.valueOf((Helpers.getCRRegisterIndex(BIOperand.getValue()) * 4) + 3),addressOperand1.getValue(),false,false,false,false,true,false);
}
