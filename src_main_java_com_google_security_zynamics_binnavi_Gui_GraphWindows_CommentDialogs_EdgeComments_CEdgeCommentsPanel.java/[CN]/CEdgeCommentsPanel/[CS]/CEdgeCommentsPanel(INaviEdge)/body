{
  super(new GridLayout(1,2,5,5));
  final GenericCommentsTable globalCommentsTable=new GenericCommentsTable(new CGlobalEdgeCommentAccessor(edge),"Global edge comment");
  final GenericCommentsTable localCommentsTable=new GenericCommentsTable(new CLocalEdgeCommentAccessor(edge),"Local edge comment");
  add(globalCommentsTable);
  add(localCommentsTable);
}
