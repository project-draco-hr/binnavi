{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"cli");
  if (instruction.getOperands().size() != 0) {
    throw new InternalTranslationException("Error: Argument instruction is not a cli instruction (invalid number of operand)");
  }
  final long baseOffset=instruction.getAddress().toLong() * 0x100;
  instructions.add(ReilHelpers.createStr(baseOffset,OperandSize.BYTE,"0",OperandSize.BYTE,Helpers.INTERRUPT_FLAG));
}
