{
  m_ParentButton.setSelected(true);
  putValue(Action.NAME,"Frozen Proximitry Browsing. Selection changes do not change graph visibility");
  putValue(Action.SHORT_DESCRIPTION,"Unfreeze Proximitry Browsing. Selection changes will change graph visibility");
  m_ParentButton.setSelectedIcon(new ImageIcon(CMain.class.getResource("data/viewlock_up.jpg")));
  m_ParentButton.setRolloverSelectedIcon(new ImageIcon(CMain.class.getResource("data/viewlock_hover.jpg")));
  m_ParentButton.setPressedIcon(new ImageIcon(CMain.class.getResource("data/viewnavi_down.jpg")));
}
