{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"ba");
  final IOperandTreeNode addressOperand1=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"ba",null,addressOperand1.getValue(),false,true,false,false,false,false);
}
