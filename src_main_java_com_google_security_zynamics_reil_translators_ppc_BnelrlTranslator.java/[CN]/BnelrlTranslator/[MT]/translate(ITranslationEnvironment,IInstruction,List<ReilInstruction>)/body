{
  TranslationHelpers.checkTranslationArguments(environment,instruction,instructions,"bnelrl");
  final IOperandTreeNode BIOperand=instruction.getOperands().get(0).getRootNode().getChildren().get(0);
  BranchGenerator.generate(instruction.getAddress().toLong() * 0x100,environment,instruction,instructions,"bnelrl",String.valueOf((Helpers.getCRRegisterIndex(BIOperand.getValue()) * 4) + 2),Helpers.LINK_REGISTER,true,false,false,false,true,true);
}
